package com.srn.erp.contabilidad.bm;

import java.util.ArrayList;
import java.util.List;

import framework.request.bl.Utils.BaseException;

public class RenglonesAsiento  {

  List renglones = new ArrayList();

  public RenglonesAsiento() {
  }

  public RenglonAsiento addRenglonAsiento() throws BaseException {
    RenglonAsiento renglonAsiento = new RenglonAsiento();
    renglones.add(renglonAsiento);
    return renglonAsiento;
  }

  public List getListaRenglones() throws BaseException {
    return renglones;
  }



}
