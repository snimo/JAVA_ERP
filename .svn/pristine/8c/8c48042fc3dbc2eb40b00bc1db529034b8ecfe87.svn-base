package com.srn.erp.general.bm;

import com.srn.erp.ventas.bm.FacturaCab;
import com.srn.erp.ventas.bm.PedidoCab;
import com.srn.erp.ventas.bm.RemitoCab;

import framework.request.bl.Utils.HashTableDatos;

public class TiposComprobantesEtiquetas {
	
	public static HashTableDatos getTipos() {
		HashTableDatos listaDatos = new HashTableDatos();
		listaDatos.put(RemitoCab.NICKNAME,"Remito");
		listaDatos.put(PedidoCab.NICKNAME,"Pedido");
		listaDatos.put(FacturaCab.NICKNAME,"Factura");
		return listaDatos;
	}

}
