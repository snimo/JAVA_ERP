package com.srn.erp.general.bm;

public class IdentificadorCompro {
	
	private String tipoCompro;
	private String letra;
	private Integer lugEmi;
	private Integer nroExt;
	
	public IdentificadorCompro (String tipoCompro,String letra, Integer lugEmi, Integer nroExt) {
		this.tipoCompro = tipoCompro;
		this.letra = letra;
		this.lugEmi = lugEmi;
		this.nroExt = nroExt;
	}
	
	public String getLetra() {
		return letra;
	}
	public void setLetra(String letra) {
		this.letra = letra;
	}
	public Integer getLugEmi() {
		return lugEmi;
	}
	public void setLugEmi(Integer lugEmi) {
		this.lugEmi = lugEmi;
	}
	public Integer getNroExt() {
		return nroExt;
	}
	public void setNroExt(Integer nroExt) {
		this.nroExt = nroExt;
	}
	public String getTipoCompro() {
		return tipoCompro;
	}
	public void setTipoCompro(String tipoCompro) {
		this.tipoCompro = tipoCompro;
	}

}
