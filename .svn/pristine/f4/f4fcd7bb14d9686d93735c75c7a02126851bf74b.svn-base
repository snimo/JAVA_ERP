package com.srn.erp.rrhh.bm;

import framework.applicarionServer.bm.Varios.Money;

public class DesgloseCalifGral {

	private String agrupador;
	private java.util.Date fecha;
	private String motivo;
	private Money puntaje;
	private Money cantidad;
	private Money ponderacion;
	private Money totPuntosPond;
	private Integer orden;
	
	public String getAgrupador() {
		return agrupador;
	}
	public void setAgrupador(String agrupador) {
		this.agrupador = agrupador;
	}
	
	public Integer getOrden() {
		return orden;
	}
	public void setOrden(Integer aOrden) {
		this.orden = aOrden;
	}
	
	
	public java.util.Date getFecha() {
		return fecha;
	}
	public void setFecha(java.util.Date fecha) {
		this.fecha = fecha;
	}
	public String getMotivo() {
		return motivo;
	}
	public void setMotivo(String motivo) {
		this.motivo = motivo;
	}
	public Money getPuntaje() {
		return puntaje;
	}
	public void setPuntaje(Money puntaje) {
		this.puntaje = puntaje;
	}
	public Money getCantidad() {
		return cantidad;
	}
	public void setCantidad(Money cantidad) {
		this.cantidad = cantidad;
	}
	public Money getPonderacion() {
		return ponderacion;
	}
	public void setPonderacion(Money ponderacion) {
		this.ponderacion = ponderacion;
	}
	public Money getTotPuntosPond() {
		return totPuntosPond;
	}
	public void setTotPuntosPond(Money totPuntosPond) {
		this.totPuntosPond = totPuntosPond;
	}

}
