          'end;'
          ''
          ''
          'procedure TraerEstadoBusqueda();'
          'begin'
          '  operacion.InicioOperacion();'
          '  operacion.setOper('#39'TraerRecluEstadoBusq'#39');'
          '  operacion.execute();'
          'end;'
          ''
          'procedure TraerSexoBusquedas();'
          'begin'
          '  operacion.InicioOperacion();'
          '  operacion.setOper('#39'TraerRecluSexoBusq'#39');'
          '  operacion.execute();'
          'end;'
          ''
          'procedure ModificarDatosBusqueda(OidBusqueda : integer);'
          'begin'
          '  operacion.InicioOperacion();'
          '  operacion.setOper('#39'TraerRecluBusqParaModif'#39');'
          '  operacion.AddAtribute('#39'oid_busqueda'#39', IntToStr(OidBusqueda));'
          '  operacion.execute();'
          'end;'
          ''
          'procedure cargarOperaciones();'
          'begin'
          '  operacion.InicioOperacion();'
          '  operacion.setOper('#39'TraerRecluOperHabAltaBusq'#39');'
          '  operacion.execute();'
          'end;')
      end>
